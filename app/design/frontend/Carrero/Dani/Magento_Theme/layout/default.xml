<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <!-- <referenceBlock name="copyright" remove="true" />-->
        <move element="copyright" destination="content" before="-"/>
        <referenceContainer name="footer">
            <block class="Magento\Cms\Block\Block" name="curso-hiberus-2021"  after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">curso-hiberus-2021</argument>
                </arguments>
            </block>
            <block class="Magento\Theme\Block\Html\Header\Logo" name="footer.logo" before="-"/>
        </referenceContainer>
        <!--<move  element="logo" destination="footer" before="-"/>-->
    </body>
</page>
