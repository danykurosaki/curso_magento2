<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/carrerob/exam/" method="GET">
        <service class="Hiberusb\Carrerob\Api\CarreroRepositoryInterface" method="getAll" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/carrerob/exam/removebyid/:entityId" method="GET">
        <service class="Hiberusb\Carrerob\Api\CarreroRepositoryInterface" method="removeByIdApi" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/carrerob/exam/create/:firstname/:lastname/:mark" method="GET">
        <service class="Hiberusb\Carrerob\Api\CarreroRepositoryInterface" method="createNew" />
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
</routes>
